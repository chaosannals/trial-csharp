{
  "Tcp": {
    "Port": 44444
  },
  "Udp": {
    "Port": 44444
  },
  "LoggingFile": {
    "PathFormat": "Logs/Server.log",
    "FileSizeLimitBytes": 2000000,
    "RetainedFileCountLimit": 100,
    "MinimumLevel": "LogLevel.Information",
    "OutputTemplate": "{Timestamp:o} {RequestId,13} [{Level:u3}] {Message} {Properties:j} ({EventId:x8}){NewLine}{Exception}",
    "IsJson": false
  },
  "ConnectionStrings": {
    "Main": "server=127.0.0.1;port=3306;database=log2demo_server;uid=root;pwd=password;CharSet=utf8mb4;SslMode=none"
  }
}